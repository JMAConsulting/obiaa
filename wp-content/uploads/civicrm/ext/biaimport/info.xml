<?xml version="1.0"?>
<extension key="biaimport" type="module">
  <file>biaimport</file>
  <name>BIA Import Extension</name>
  <description>BIA Import Extension to hold APIs</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Seamus Lee</author>
    <email>seamus.lee@jmaconsulting.biz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://lab.jmaconsulting.biz/extensions/obiaa/biaimport</url>
    <url desc="Documentation">https://lab.jmaconsulting.biz/extensions/obiaa/biaimport</url>
    <url desc="Support">https://lab.jmaconsulting.biz/extensions/obiaa/biaimport</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-07-26</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <requires>
    <ext>biaproperty</ext>
    <ext>nz.co.fuzion.csvimport</ext>
  </requires>
  <civix>
    <namespace>CRM/Biaimport</namespace>
    <format>23.02.1</format>
    <angularModule>crmBiaimport</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>

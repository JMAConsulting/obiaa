<?xml version="1.0"?>
<extension key="stripedisablepostcode" type="module">
  <file>stripedisablepostcode</file>
  <name>Disable Postcode on stripe card</name>
  <description>Disables the Postcode Field on stripe card object by overriding the civicrm_stripe.js file with a customised version</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Seamus Lee</author>
    <email>seamus.lee@jmaconsulting.biz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://lab.jmaconsulting.biz/extensions/stripedisablepostcode</url>
    <url desc="Documentation">https://lab.jmaconsulting.biz/extensions/stripedisablepostcode</url>
    <url desc="Support">https://lab.jmaconsulting.biz/extensions/stripedisablepostcode</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-08-02</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <requires>
    <ext>com.drastikbydesign.stripe</ext>
  </requires>
  <civix>
    <namespace>CRM/Stripedisablepostcode</namespace>
    <format>22.05.2</format>
    <angularModule>crmStripedisablepostcode</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>

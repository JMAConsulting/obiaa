<?xml version="1.0"?>
<extension key="biamailingreplyto" type="module">
  <file>biamailingreplyto</file>
  <name>biamailingreplyto</name>
  <description>BIA Mailing Reply to extension sets the reply to of CiviMails to be the primary contact email</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Seamus Lee</author>
    <email>seamus.lee@jmaconsulting.biz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://lab.jmaconsulting.biz/extensions/obiaa/biamailingreplyto</url>
    <url desc="Documentation">https://lab.jmaconsulting.biz/extensions/obiaa/biamailingreplyto</url>
    <url desc="Support">https://lab.jmaconsulting.biz/extensions/obiaa/biamailingreplyto</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-12-12</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Biamailingreplyto</namespace>
    <format>22.10.0</format>
    <angularModule>crmBiamailingreplyto</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
